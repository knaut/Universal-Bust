{
  "name": "universal-bust",
  "version": "2.0.0",
  "description": "a boilerplate project for server-side rendered, single-page apps",
  "main": "index.js",
  "scripts": {
    "start": "npm run app:local",
    "app:local": "webpack-dev-server --config webpack.local.js",
    "app:graph": "npx madge -i madge_app_graph.svg ./app",
    "server:build": "npx babel -d ./server_build ./server -s",
    "server:start": "npx @babel/node ./server_build",
    "server:watch": "npx babel ./server --watch --out-file server.build.js & nodemon -L server.build.js",
    "server:graph": "npx madge -i madge_server_graph.svg ./server",
    "standard": "standard",
    "standardize": "standard --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hapi/glue": "^6.1.0",
    "@hapi/good": "^8.2.0",
    "@hapi/good-console": "^8.1.0",
    "@hapi/good-squeeze": "^5.2.0",
    "@hapi/hapi": "^18.3.1",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "figlet": "^1.2.3",
    "madge": "^3.4.4",
    "moment": "^2.24.0",
    "node-emoji": "^1.10.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.9",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^5.0.3",
    "deepmerge": "^3.2.1",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "standard": "^12.0.1",
    "storybook-react-router": "^1.0.5",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
